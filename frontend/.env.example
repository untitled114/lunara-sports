# Firebase Configuration
# Copy this file to .env and fill in your values

# Firebase API Key
VITE_FIREBASE_API_KEY=your_firebase_api_key_here

# Firebase Auth Domain
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com

# Firebase Project ID
VITE_FIREBASE_PROJECT_ID=your_project_id_here

# Application ID (for Firestore paths: artifacts/{APP_ID}/public/data/messages)
VITE_APP_ID=lunara-default

# Initial Auth Token (optional - falls back to anonymous sign-in if not provided)
# Set via window.__initial_auth_token in production
